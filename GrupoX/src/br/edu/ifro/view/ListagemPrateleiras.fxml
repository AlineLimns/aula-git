<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="br.edu.ifro.control.ListagemPrateleiraController">
    <children>
      <TableView fx:id="tbPrateleiras" layoutX="30.0" layoutY="62.0" prefHeight="212.0" prefWidth="422.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Id">
            <cellValueFactory>
                <PropertyValueFactory property="id" />
            </cellValueFactory>
          </TableColumn> 
          <TableColumn prefWidth="103.0" text="Numero">
              <cellValueFactory>
                <PropertyValueFactory property="numero" />
            </cellValueFactory>
          </TableColumn>
            <TableColumn prefWidth="103.0" text="Tipo">
               <cellValueFactory>
                  <PropertyValueFactory property="tipo" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="140.0" text="Observações">
               <cellValueFactory>
                  <PropertyValueFactory property="observacoes" />
               </cellValueFactory>
            </TableColumn> 
        </columns>
      </TableView>
      <Button layoutX="462.0" layoutY="94.0" mnemonicParsing="false" onAction="#excluir" text="Excluir" />
      <Button layoutX="462.0" layoutY="126.0" mnemonicParsing="false" onAction="#editar" text="Editar" />
      <Button fx:id="btnFechar" layoutX="237.0" layoutY="306.0" mnemonicParsing="false" onAction="#fechar" prefHeight="38.0" prefWidth="62.0" text="Fechar" />
   </children>
</AnchorPane>
